{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/react-src/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "chrome": {
      "title": "Mercator",
      "width": 800,
      "height": 500
    }
  },
  "cli": {
    "binaryName": "Mercator",
    "resourcesPath": "/react-src/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/react-src/public/neutralino.js",
    "binaryVersion": "4.4.0",
    "clientVersion": "3.3.0",
    "frontendLibrary": {
      "patchFile": "/react-src/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}